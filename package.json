{
  "name": "beispiel",
  "version": "1.0.0",
  "description": "Beispiel",
  "main": "dist/index.js",
  "engines": {
    "node": ">=13.11.0",
    "npm": ">=6.14.3"
  },
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "npm run init && nodemon --exec ts-node src/index.ts --unhandled-rejections=strict",
    "start:debug": "npm run prepare && nodemon --inspect dist/index.js ",
    "pretest": "npm run init && if exist server.log del server.log",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "npm t -- --coverage",
    "asciidoc": "ts-node scripts/asciidoc",
    "clean": "ts-node scripts/clean",
    "curl:heroku": "C:\\Zimmermann\\Git\\mingw64\\bin\\curl --include --tlsv1.2 --insecure https://gener-iert-12345.herokuapp.com/buecher/00000000-0000-0000-0000-000000000001",
    "curl:openshift": "C:\\Zimmermann\\Git\\mingw64\\bin\\curl --include http://beispiel-jz.apps.us-east-1.starter.openshift-online.com:8443/buecher/00000000-0000-0000-0000-000000000001",
    "depcheck": "depcheck",
    "deploy:heroku": "git push heroku master",
    "deploy:openshift": "npm run deploy:prepare && npx nodeshift --expose --deploy.port 8443",
    "deploy:prepare": "concurrently --prefix \"[{name}]\" --names \"ESLint,ejs-lint,init,TypeScript\" --prefix-colors \"yellow.bold,gray.bold,cyan.bold,green.bold\" \"npm run eslint\" \"npm run ejs-lint\" \"npm run init\" \"npm run tsc\"",
    "ejs-lint": "node ./node_modules/ejs-lint/index.js",
    "eslint": "npx eslint src/**/*.ts",
    "init": "ts-node scripts/init",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "mongo": "ts-node scripts/mongo",
    "open:heroku": "heroku open --app gener-iert-12345 /buecher/00000000-0000-0000-0000-000000000001",
    "open:heroku:file": "heroku open --app gener-iert-12345 /buecher/00000000-0000-0000-0000-000000000001/file",
    "prettier": "npx prettier --write src/**/*.ts scripts/*.?s test/**/*.ts",
    "heroku-postbuild": "npm run tsc && node scripts/init.heroku",
    "tsc": "npx tsc"
  },
  "files": [
    "dist",
    "package.json",
    ".env",
    ".gitignore",
    ".npmrc",
    "tsconfig.json",
    "scripts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint",
      "pre-push": "npm t"
    }
  },
  "private": true,
  "homepage": "https://www.hs-karlsruhe.de",
  "pre-commit": [
    "npm run eslint"
  ],
  "keywords": [
